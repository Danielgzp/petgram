{
    "version": 2,
    "name": "danielgzp01-app",
    "builds": [
        {
            "use": "@vercel/static",
            "src": "package.json"
        }
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}
